<div class="team">
    <h2 class="team-title">Our team</h2>
    <ul class="team-list">
        {{#each this}}
        <li class="team-member">
            <img class="team-member-img" src="{{}this.url}" alt="photo of {{this.name}}">
            <h3 class="team-member-name">{{this.name}}</h3>
            <a class="team-member-link">GitHub: {{this.html_url}}</a>
        </li>
        {{/each}}
    </ul>
</div>